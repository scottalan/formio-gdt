{
  "srcPaths": {
    "make": "src/project.make",
    "drupal": "src"
  },
  "domain": "formio.dev",
  "packages": {
    "srcFiles": [
      "!sites/*/files/**",
      "!xmlrpc.php",
      "!modules/php/*"
    ],
    "projFiles": [
      "README*",
      "bin/**"
    ]
  },
  "phpmd": {
    "path": "vendor/bin/phpmd"
  },
  "drush": {
    "cmd": "vendor/bin/drush"
  },
  "behat": {
    "flags": "--tags ~@wip"
  },
  "eslint": true,
  "scripts": {
    "update": "<%= config.drush.cmd %> <%= config.alias %> features-revert-all -yv"
  },
  "project": {
    "profile": "standard"
  },
  "generated": {
    "name": "generator-gadget",
    "version": "0.4.0"
  }
}
